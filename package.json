{
  "name": "pg300-quiz-exam",
  "version": "1.0.0",
  "description": "PG6300 exam",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node server/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools tests server",
    "lint:watch": "npm run lint -- --watch",
    "testBabel": "mocha --compilers js:babel-core/register --require babel-polyfill",
    "test": "mocha --reporter spec tools/testSetup.js tests/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "jest": "jest --coverage",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node server/distServer.js"
  },
  "author": "Stella",
  "license": "ISC",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.1.3",
    "cors": "2.8.4",
    "jquery": "3.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router-bootstrap": "0.24.4",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "colors": "1.3.2",
    "compression": "1.7.3",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.1",
    "eslint": "5.4.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.26.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "eventsource-polyfill": "0.9.6",
    "expect": "23.5.0",
    "express": "4.16.3",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-enzyme": "7.0.0",
    "jest-runner-mocha": "0.6.0",
    "jsdom": "11.11.0",
    "mocha": "5.2.0",
    "nock": "9.6.1",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "16.4.2",
    "redux-devtools-extension": "2.13.5",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "style-loader": "0.23.0",
    "supertest": "3.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "3.2.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "2.23.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.test\\.(js|jsx)$",
    "collectCoverageFrom": [
      "src/**.(js|jsx)"
    ],
    "runner": "jest-runner-mocha"
  }
}
